- pages ||= @news_pages || []

%h1.what_is_new= translate :what_is_new_in_my_groups
%div.what_is_new
  %ul
    - for page in pages
      %li
        %strong>= link_to (page.title.present? ? page.title : translate(:untitled)), page
        , 
        = localize page.updated_at.to_date